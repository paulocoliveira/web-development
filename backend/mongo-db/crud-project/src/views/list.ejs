<!DOCTYPE html>
<html lang="en">
    <%- include("partials/head.ejs") %> 
<body>
    <h1><%= title %></h1>
    
    <%- include("partials/navbar.ejs") %> 

    <ul>
        <% users.forEach( (user) => { %>
            <li>
                <%= user.name %> - <%= user.email %> - <%= user.age %> anos - 
                <a href="/edit?id=<%= user._id %>">[edit]</a> - 
                <a href="/remove/<%= user._id %>">[remove]</a>
            </li>
        <% }) %> 
    </ul>

    <%- include("partials/foot.ejs") %> 
</body>
</html>